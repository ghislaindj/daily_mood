<div class="panel panel-default">
  <div class="panel-heading">My Mood</div>
  <div class="panel-body">
    <div class="col-md-2" id="my-mood">
      <div class="mood">
        <svg id="my-smiley" version="1.1" xmlns="http://www.w3.org/2000/svg" height="100" width="100" onload="smiley('#my-smiley', <%= @last_mood.value %>);">
        </svg>
        <legend>Today</legend>
      </div>
    </div>
    <div class="col-md-3" id="pie">
    </div>
    <div class="col-md-7" id="line"></div>
  </div>
</div>

<%= render partial: "pie" %>
<%= render partial: "line_chart" %>